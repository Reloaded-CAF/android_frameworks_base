<resources>

  <!-- The list of components which should be automatically disabled. -->
  <string-array name="config_disabledComponents" translatable="false"/>

  <!-- Power menu -->
  <java-symbol type="string" name="system_error_vendorprint" />

  <java-symbol type="array" name="config_blockPackagesSensorDrain" />

  <java-symbol type="array" name="config_vendorPlatformSignatures" />

  <!-- Screenrecord -->
  <java-symbol type="bool" name="config_enableScreenrecordChord" />

  <!-- Power menu reboot -->
  <java-symbol name="reboot_to_bootloader_title" type="string" />
  <java-symbol name="reboot_to_bootloader_message" type="string" />
  <java-symbol name="reboot_to_recovery_title" type="string" />
  <java-symbol name="reboot_to_recovery_message" type="string" />
  <java-symbol name="reboot_system_title" type="string" />
  <java-symbol name="reboot_system_message" type="string" />
  <java-symbol type="array" name="config_rebootActionsList" />
  <java-symbol type="array" name="values_globalActionsList" />

  <!-- Key handler -->
  <java-symbol type="string" name="config_deviceKeyHandlerLib" />
  <java-symbol type="string" name="config_deviceKeyHandlerClass" />

  <!-- True if the notification's should dynamically tint the app icon and text -->
  <java-symbol type="bool" name="config_allowNotificationIconTextTinting" />
  <!-- True if the notification's dynamic app icon and text tint should be optimised for dark
       backgrounds -->
  <java-symbol type="bool" name="config_useDarkBgNotificationIconTextTinting" />

  <!-- Notification icon tint -->
  <java-symbol type="color" name="notification_icon_default_color" />
  <!-- Notification text tint -->
  <java-symbol type="color" name="notification_text_default_color" />
  <!-- Ambient notification icon tint -->
  <java-symbol type="color" name="notification_ambient_icon_default_color" />

  <!-- Exposed resolver style -->
  <java-symbol type="style" name="Theme.DeviceDefault.Resolver.Exposed" />
  <!-- Exposed resolver color -->
  <java-symbol type="color" name="chooser_service_row_background_color_exposed" />

  <!-- Leds -->
  <java-symbol type="bool" name="config_intrusiveBatteryLed" />

  <!-- Whether to cleanup fingerprints upon connection to the daemon and when user switches -->
  <java-symbol type="bool" name="config_cleanupUnusedFingerprints" />

  <!-- Whether notify fingerprint client of successful cancelled authentication -->
  <java-symbol type="bool" name="config_notifyClientOnFingerprintCancelSuccess" />

  <java-symbol type="integer" name="config_flashlightProximityTimeout" />
  
  <!-- Performance Profiles -->
  <java-symbol type="bool" name="config_hasPerformanceProfiles" />

</resources>
